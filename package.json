{
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../backend && npm install",
    "start:frontend": "cd client && npm run serve",
    "start:backend": "cd backend && npm run server",
    "start:all": "npm-run-all -p install:all start:frontend start:backend"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}